{
  "name": "cetriolino",
  "version": "0.3.4",
  "description": "Simple key-value database",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "watch": "rollup -c rollup.config.js -w",
    "test": "jest",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "docs": "typedoc --theme minimal --mode file --entryPoint Cetriolino --out docs/  src/"
  },
  "author": "Alessandro Cifani",
  "license": "MIT",
  "repository": "github:acifani/cetriolino",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^11.9.0",
    "auto-changelog": "^1.10.2",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "rollup": "^1.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript2": "^0.19.0",
    "ts-jest": "^23.10.4",
    "typedoc": "^0.14.0",
    "typescript": "^3.1.6"
  }
}
